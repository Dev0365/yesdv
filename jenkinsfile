pipeline () {
   agent any 
 stages {
   stage('docker image'){
    steps{
   sh "docker build -t kar_image . "
   }
   }
   stage('docker run'){
      steps{
      sh  
         '''
          " docker volume create kar "
          "docker run -d --name k1 -v kar -p 5000:80 kar_image /bin/bash"
          '''
      }
      }
      
   }
   }
   
